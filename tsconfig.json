{
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react-native",
    "module": "esnext",
    "target": "esnext",
    "lib": ["es2019"],
    "allowJs": true,
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true
  },
  "exclude": [
    "node_modules", "babel.config.js", "metro.config.js", "jest.config.js"
  ]
}